{
  "name": "reactnativeapitask",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 15\"",
    "ios_8": "react-native run-ios --simulator \"iPhone 8\"",
    "iPad_Pro_5_Gen": "react-native run-ios --simulator \"iPad Pro (12.9-inch) (5th generation)\"",
    "iPhone_14_Pro_Max": "react-native run-ios --simulator \"iPhone 14 Pro Max\"",
    "iPhone_15_Pro_Max": "react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "iPhone_SE_3": "react-native run-ios --simulator \"iPhone SE (3rd generation)\"",
    "iPhone_Xs": "react-native run-ios --simulator \"iPhone Xs\"",
    "iPhone_12": "react-native run-ios --device \"iPhone 12\"",
    "pod_install": "cd ios && pod install && cd ..",
    "open-debug-folder": "open android/app/build/outputs/apk/debug",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "makeApk": "npm run gClean && npm run bundle-android && npm run debug && npm run open-debug-folder",
    "debug": "cd android && ./gradlew assembleDebug && cd ..",
    "gClean": "cd android && ./gradlew clean && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "log_release": "npx react-native log-android",
    "android_release": "npx react-native run-android --variant=release",
    "basicSetup": "yarn add redux react-redux validator redux-thunk redux-persist react-native-size-matters react-native-safe-area-context react-native-screens react-native-localization react-native-bootsplash moment @react-native-async-storage/async-storage @react-native-community/netinfo @react-navigation/native @react-navigation/native-stack axios react-native-toast-message react-native-device-info react-native-permissions @reduxjs/toolkit @react-native-firebase/crashlytics @react-native-community/push-notification-ios react-native-push-notification @react-native-firebase/messaging react-native-keyboard-aware-scroll-view react-native-vector-icons react-native-svg react-native-svg-transformer"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.3.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.6.7",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-bootsplash": "^5.4.0",
    "react-native-device-info": "^10.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localization": "^2.3.2",
    "react-native-permissions": "^4.1.2",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.2.0",
    "react-native-toast-message": "^2.1.9",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
